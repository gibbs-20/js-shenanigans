imurl = 'data:image/png;base64,';
const images = document.querySelectorAll('img');

for (const image of images) {
  // Set the new image source or alt attribute if it's currently empty
  if (!image.src.endsWith('.jpg') && !image.src.endsWith('.png') && !image.src.endsWith('.gif')) {
    if (!image.alt) {
      image.alt = imurl;
    }
  } else {
    // Set the new image source only if it's initially an image (not a placeholder)
    if (image.src.endsWith('.jpg') || image.src.endsWith('.png') || image.src.endsWith('.gif')) {
      image.src = imurl;
    }
  }
}

console.log('Done!');
